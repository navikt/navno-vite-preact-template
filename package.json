{
  "name": "navno-vite-preact-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:client": "vite build --outDir ./server/dist/client",
    "build:ssr": "vite build --outDir ./server/src/_ssr-dist --ssr src/main-server.tsx",
    "build:server": "npm run build --workspace server",
    "start": "npm run start --workspace server",
    "decorator": "docker compose up",
    "dev": "npm run dev --workspace server",
    "prod-local": "cp .env.local-prod .env && npm run prod-local --workspace server",
    "test": "jest"
  },
  "workspaces": ["server"],
  "dependencies": {
    "@navikt/ds-css": "2.4.0",
    "@navikt/ds-react": "2.4.0",
    "@navikt/nav-dekoratoren-moduler": "1.9.0",
    "@preact/compat": "17.1.2",
    "preact-render-to-string": "5.2.6"
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@preact/preset-vite": "2.5.0",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint": "8.34.0",
    "eslint-plugin-react": "7.32.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "vite": "4.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

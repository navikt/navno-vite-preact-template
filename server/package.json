{
  "name": "navno-vite-preact-template-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ../dist/server/server.js",
    "start-local": "npm run build && concurrently \"tsc -w\" \"nodemon -w ../dist/server ../dist/server/server.js\"",
    "prod-local": "cp .env.local-prod .env && npm run start-local",
    "dev": "cp .env.development .env && npm run start-local"
  },
  "dependencies": {
    "compression": "1.7.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "lru-cache": "7.14.1",
    "vite": "4.1.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/lru-cache": "7.10.10",
    "@types/node": "18.11.18",
    "concurrently": "7.6.0",
    "nodemon": "2.0.20"
  }
}

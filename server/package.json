{
  "name": "navno-vite-preact-template-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server/server.js",
    "dev": "npm run build && concurrently \"tsc -w\" \"nodemon -w ./dist/server ./dist/server/server.js\""
  },
  "dependencies": {
    "compression": "1.7.4",
    "csp-header": "5.1.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "lru-cache": "7.14.1",
    "node-cache": "5.1.2"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/node": "18.11.18",
    "concurrently": "7.6.0",
    "nodemon": "2.0.20"
  },
  "nodemonConfig": {
    "delay": 500
  }
}
